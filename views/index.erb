<div id="home_menus">

<div class="list">
<h2>Your Homepages</h2>

<ul>
	<% @homepages.each do |homepage| %>
		<li><a href="<%= homepage.id %>"><%= homepage.title %></a> <span class="edit"><a class="destroy" href="/homepage/<%= homepage.id %>/destroy">&#9746;</a> <a href="/homepage/<%= homepage.id %>/edit">&#9998;</a></span></li>
	<% end %>
		<li><a href="/homepage/new">+1</a></li>
</ul>
</div>

<div class="list">
<h2>Your Feeds</h2>
<ul>	
	<% @feeds.each do |feed| %>
	<li><a href="<%= feed.url %>"><%= feed.title %></a> <span class="edit"><a class="destroy" href="/feed/<%= feed.id %>/destroy">&#9746;</a> <a href="/feed/<%= feed.id %>/edit">&#9998;</a></span></li>
	<% end %>
	<li><a href="/feed/new">+1</a></li>
</ul>
</div>
</div>